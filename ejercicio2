class Animalia:
    pass

class Animal(Animalia):
    pass

class Mamifero(Animalia):
    pass

class Oviparo(Animalia):
    pass

class Pollo(Oviparo):
    pass

class Gato(Mamifero):
    pass

class Ornitorrinco(Mamifero, Oviparo):
    pass


#Clase Animalia
# - Propiedades:
#  - Ninguna
# - Métodos:
#   - Ninguno

